<script>
    $(document).ready(function () {
        $(".r-tabs-anchor").click(function (e) {
            e.preventDefault();
            $(this).parent().find("li").removeClass('r-tabs-state-active');
            setTimeout(function () {
                $(this).parent().find("li").addClass('saving');
            }, 2000);
            $(".pane").css('display', 'none');
            var toShow = $(this).attr('href');
            $(toShow).show();
        });

        $('.marker').click(function (e) {
            e.preventDefault();
            $('.hotspot.electricBlue.active').removeClass('active');
            $(this).parent().addClass('active');
        });
        $('.close').click(function (e) {
            e.preventDefault();
            $('.hotspot.electricBlue.active').removeClass('active');
        });
    });
</script>